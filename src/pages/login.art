<body>
    <header>
        <a href="./index.html">
            <img src="../images/logoLogin.png" />
        </a>
        <p class="platform">健康家居全生活化服务平台</p>
    </header>
    <main>
        <div class="mainbox">
            <form>
                <p class="title">
                    <span>账户登录</span>
                    <!-- <span>扫码登录</span> -->
                </p>
                <p class="tip"></p>
                <p class="input name">
                    <span>
                        <img src="../images/account.png" alt="">
                    </span>
                    <input maxlength="21" autocomplete="off" type="text" id="username" placeholder="手机号/账号" />
                    <span class="clear n">
                        <img src="../images/Clear.png" />
                    </span>
                </p>

                <p class="input psw">
                    <span>
                        <img src="../images/pw.png" />
                    </span>
                    <input maxlength="18" autocomplete="off" type="password" id="password" placeholder="密码" />
                    <span class="clear p">
                        <img src="../images/Clear.png" />
                    </span>
                </p>
                <div class="safe">
                    <a href="findPassWord.html" class="forget">忘记密码?</a>
                    <a href="register.html" class="register">注册账号</a>
                </div>
                <p id="login">
                    <span>登录</span>
                    <span class="ellipses"></span>
                </p>
